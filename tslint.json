{
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-shadowed-variable": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "ban-types": false,
    "trailing-comma": false,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-string-literal": false,
    "no-console": true,
    "prefer-for-of": false,
    "interface-name": [
      false
    ]
  },
  "rulesDirectory": []
}
